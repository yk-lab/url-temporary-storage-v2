{
  "name": "url-temporary-storage",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-06-21 ./public --kv=SESSION_KV",
    "typecheck": "tsc",
    "pages:deploy": "pnpm run build && wrangler pages deploy ./public",
    "migrations:gen": "drizzle-kit generate:sqlite",
    "local:migrations:apply": "wrangler d1 migrations apply DB --local",
    "migrations:apply": "NO_D1_WARNING=true wrangler d1 migrations apply url-temporary-storage"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.1.0",
    "@remix-run/cloudflare-pages": "2.14.0",
    "@remix-run/css-bundle": "2.1.0",
    "@remix-run/react": "2.1.0",
    "isbot": "3.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231025.0",
    "@heroicons/react": "2.0.18",
    "@remix-run/dev": "2.1.0",
    "@remix-run/eslint-config": "2.1.0",
    "@remix-validated-form/with-zod": "2.0.7",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "@types/bcryptjs": "2.4.5",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "bcryptjs": "2.4.3",
    "better-sqlite3": "9.0.0",
    "burner-email-providers": "1.0.67",
    "cheerio": "1.0.0-rc.12",
    "drizzle-kit": "0.19.13",
    "drizzle-orm": "0.28.6",
    "drizzle-zod": "0.5.1",
    "eslint": "8.52.0",
    "i18next": "23.6.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.6",
    "pretty-bytes": "6.1.1",
    "remix-auth": "3.6.0",
    "remix-auth-email-link": "2.1.0",
    "remix-auth-form": "1.4.0",
    "remix-validated-form": "5.1.5",
    "studio": "0.13.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "wrangler": "3.15.0",
    "zod": "3.22.4",
    "zod-i18n-map": "2.21.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
